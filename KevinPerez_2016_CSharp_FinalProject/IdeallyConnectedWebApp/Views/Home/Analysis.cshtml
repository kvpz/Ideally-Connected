@{
    ViewBag.Title = "Analysis One";
}
@section scripts {
   @*<script src="~/lib/angular/angular.js"></script> *@
    <script src="~/App/AnalysisOne/App.js"></script>
    <script src="~/App/AnalysisOne/ViewModels/RootViewModel.js"></script>
    <script src="~/App/AnalysisOne/ViewModels/AnalysisHomeViewModel.js"></script>
    <script src="~/App/AnalysisOne/ViewModels/AnalysisListViewModel.js"></script>
    <script src="~/App/AnalysisOne/ViewModels/AnalysisViewModel.js"></script>
}

@section jsCode {
    @* Manual initialization. usage: angular.bootstrap(element, [modules], [config])*@
    angular.bootstrap(document.getElementById("analysis"), ['analysis']);
}

<div ng-non-bindable>
    <div id="analysis" data-ng-controller="rootViewModel">
        <h2>{{ pageHeading }}</h2>
        <div class="row">
            <div class="col-md-4">
                <button class="btn btn-primary" ng-click="analysisList()">Analysis List</button>
            </div>
        </div>
        <div class="col-md-4">
            <button class="btn btn-primary" ng-click="showAnalysis()">Analysis</button>&nbsp;
            <input type="text" ng-model="analysisService.analysisID" />
        </div>
    </div>

    <hr/>
    <div ng-view></div>
    <hr/>
    <div>
        
        
    </div>
</div>